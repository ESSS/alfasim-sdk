            hook_update_variables_point -> tracer_3
            tracer_3 -> hook_calculate_source_terms_point [arrowhead=none]
        }

        edge[constraint=false, style=dotted, color="#DA5961"]

        subgraph cluster2{
            label="Hooks for User Defined Tracers"
            labeljust="l"
            fontcolor="#2c3e50"
            style="rounded, dashed"
            color="#8699A3"

