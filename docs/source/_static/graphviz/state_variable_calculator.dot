        node [shape=point style=invis width=0]
        invisible_init
        invisible_end
        invisible_1
        invisible_2
        invisible_3

        node[peripheries="0" shape="cds", color="#DA5961",  fontcolor="#DA5961" , style="" target="_top" fontsize=10]
