name: Simple Case
physics:
  hydrodynamic_model: hydrodynamic_model_4_fields
  simulation_regime: simulation_regime_transient
  energy_model: no_model
  solids_model: no_model
  solids_model_plugin_id:
  initial_condition_strategy: constant
  keep_former_results: False
  emulsion_model_enabled: True
  emulsion_relative_viscosity_model: brinkman1952
  emulsion_pal_rhodes_phi_rel_100:
    value: 0.765
    unit: '-'
  emulsion_woelflin_a:
    value: 4.2
    unit: '-'
  emulsion_woelflin_b:
    value: 2.5
    unit: '-'
  emulsion_table_based_rel_visc_curve:
    image:
      values:
      - 1.0
      unit: '-'
    domain:
      values:
      - 0.0
      unit: m3/m3
  emulsion_relative_viscosity_tuning_factor:
    image:
      values:
      - 1.0
      unit: '-'
    domain:
      values:
      - 0.0
      unit: m3/m3
  emulsion_droplet_size_model: brauner2001
  emulsion_inversion_point_model: brauner_and_ullmann_2002
  emulsion_inversion_water_cut:
    value: 0.4
    unit: m3/m3
  friction_factor_correlation_model: model_default
  friction_factor_correlation_plugin_id:
  flash_model: hydrocarbon_and_water
  correlations_package: correlation_package_classical
  fluid_material_convection_correlation: Hasan and Kabir 1994
  steady_state_guess_initialization: quasi_hydrostatic
time_options:
  stop_on_steady_state: False
  automatic_restart_autosave_frequency: True
  initial_time:
    value: 0.0
    unit: s
  final_time:
    value: 10.0
    unit: s
  initial_timestep:
    value: 0.0001
    unit: s
  minimum_timestep:
    value: 1e-12
    unit: s
  maximum_timestep:
    value: 0.1
    unit: s
  restart_autosave_frequency:
    value: 1.0
    unit: h
  minimum_time_for_steady_state_stop:
    value: 0.0
    unit: s
numerical_options:
  nonlinear_solver_type: nonlinear_solver_alfasim_quasi_newton
  tolerance: 0.0001
  maximum_iterations: 5
  maximum_timestep_change_factor:
    value: 2.0
    unit: '-'
  maximum_cfl_value:
    value: 1.0
    unit: '-'
  relaxed_tolerance: 0.0
  divergence_tolerance: -1.0
  friction_factor_evaluation_strategy: time_explicit
  simulation_mode: default
  enable_solver_caching: True
  caching_rtol: 0.01
  caching_atol: 0.0001
  always_repeat_timestep: False
  enable_fast_compositional: True
plugins: []
outputs:
  automatic_trend_frequency: True
  trends:
    positional_pipe_trends: []
    overall_pipe_trends: []
    global_trends: []
    equipment_trends: []
    separator_trends: []
    controller_trends: []
  trend_frequency:
    value: 0.1
    unit: s
  automatic_profile_frequency: True
  profiles: []
  profile_frequency:
    value: 0.1
    unit: s
pipes:
- name: pipe
  source: mass_source_inlet
  target: pressure_outlet
  profile:
    x_and_y:
      x:
        exprs:
        - A
        - 2.0
        - A+B
        - 4.0
        unit: m
      y:
        exprs:
        - A
        - 2.0
        - A+B
        - 4.0
        unit: m
  environment:
    thermal_model: steady_state_heat_transfer
    position_input_mode: position_by_md
    reference_y_coordinate:
      value: 0.0
      unit: m
    md_properties_table: []
    tvd_properties_table: []
  segments:
    start_positions:
      values:
      - 0.0
      unit: m
    diameters:
      values:
      - 0.1
      unit: m
    roughnesses:
      values:
      - 1e-05
      unit: m
  initial_conditions:
    pressures:
      position_input_type: length
      table_x:
        reference_coordinate:
          value: 0.0
          unit: m
        positions:
          values:
          - 0.0
          unit: m
        pressures:
          values:
          - 100000.0
          unit: Pa
      table_y:
        reference_coordinate:
          value: 0.0
          unit: m
        positions:
          values:
          - 0.0
          unit: m
        pressures:
          values:
          - 100000.0
          unit: Pa
      table_length:
        positions:
          values:
          - 0.0
          unit: m
        pressures:
          values:
          - 100000.0
          unit: Pa
    volume_fractions:
      position_input_type: length
      table_x:
        reference_coordinate:
          value: 0.0
          unit: m
        positions:
          values: []
          unit: m
      table_y:
        reference_coordinate:
          value: 0.0
          unit: m
        positions:
          values: []
          unit: m
      table_length:
        positions:
          values:
          - 0.0
          unit: m
        fractions:
          gas:
            values:
            - 0.1
            unit: '-'
          oil:
            values:
            - 0.9
            unit: '-'
    tracers_mass_fractions:
      position_input_type: length
      table_x:
        reference_coordinate:
          value: 0.0
          unit: m
        positions:
          values: []
          unit: m
        tracers_mass_fractions: []
      table_y:
        reference_coordinate:
          value: 0.0
          unit: m
        positions:
          values: []
          unit: m
        tracers_mass_fractions: []
      table_length:
        positions:
          values: []
          unit: m
        tracers_mass_fractions: []
    velocities:
      position_input_type: length
      table_x:
        reference_coordinate:
          value: 0.0
          unit: m
        positions:
          values: []
          unit: m
      table_y:
        reference_coordinate:
          value: 0.0
          unit: m
        positions:
          values: []
          unit: m
      table_length:
        positions:
          values:
          - 0.0
          unit: m
        velocities:
          gas:
            values:
            - 1e-08
            unit: m/s
          oil:
            values:
            - 1e-08
            unit: m/s
    temperatures:
      position_input_type: length
      table_x:
        reference_coordinate:
          value: 0.0
          unit: m
        positions:
          values: []
          unit: m
        temperatures:
          values: []
          unit: K
      table_y:
        reference_coordinate:
          value: 0.0
          unit: m
        positions:
          values: []
          unit: m
        temperatures:
          values: []
          unit: K
      table_length:
        positions:
          values:
          - 0.0
          unit: m
        temperatures:
          values:
          - 288.6
          unit: K
  flow_pattern_model: unit_cell
  regime_capturing_mesh_threshold:
    value: 0.0
    unit: '-'
nodes: []
wells: []
materials: []
walls: []
